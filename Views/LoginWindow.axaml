<Window
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignWidth="1920"
    d:DesignHeight="1080"
    x:Class="miniJogo.Views.LoginWindow"
    x:CompileBindings="True"
    Title="Mini Jogo LEDs - Login"
    Width="1920"
    Height="1080"
    WindowStartupLocation="CenterScreen"
    WindowState="FullScreen"
    CanResize="True"
    Background="#0F0F23"
    KeyDown="Window_KeyDown"
    ExtendClientAreaToDecorationsHint="True">

    <!-- Layout corrigido para evitar sobreposi√ß√µes -->
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <!-- Header - Mantido compacto -->
        <Border Grid.Row="0" Padding="20">
            <Border.Background>
                <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,0%">
                    <GradientStop Color="#5E60CE" Offset="0"/>
                    <GradientStop Color="#7400B8" Offset="1"/>
                </LinearGradientBrush>
            </Border.Background>
            <StackPanel HorizontalAlignment="Center" Spacing="8">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="15">
                    <TextBlock Text="üéÆ" FontSize="40" VerticalAlignment="Center" />
                    <TextBlock
                        Text="Mini Jogo LEDs - Arduino Gaming"
                        FontSize="38"
                        FontWeight="Bold"
                        Foreground="White">
                        <TextBlock.Effect>
                            <DropShadowEffect BlurRadius="8" OffsetX="0" OffsetY="2" Color="#30000000"/>
                        </TextBlock.Effect>
                    </TextBlock>
                </StackPanel>
                <TextBlock
                    Text="Sistema de Autentica√ß√£o e Sele√ß√£o de Jogos"
                    FontSize="20"
                    Foreground="#E2E8F0"
                    Opacity="0.95"
                    HorizontalAlignment="Center"
                />
            </StackPanel>
        </Border>

        <!-- Conte√∫do Principal - Layout adaptativo -->
        <Grid Grid.Row="1" Margin="20">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="2*" MinWidth="500" />
                <ColumnDefinition Width="3*" MinWidth="700" />
            </Grid.ColumnDefinitions>

            <!-- Painel Esquerdo - Login Form -->
            <Border
                Grid.Column="0"
                CornerRadius="20"
                Margin="0,0,15,0">
                <Border.Background>
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#1E1E3F" Offset="0"/>
                        <GradientStop Color="#2A2A5A" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
                <Border.Effect>
                    <DropShadowEffect BlurRadius="20" OffsetX="0" OffsetY="10" Color="#30000000"/>
                </Border.Effect>

                <!-- Layout adaptativo usando DockPanel -->
                <DockPanel LastChildFill="True" Margin="25">
                    <!-- Logo fixo no topo -->
                    <Image
                        Source="/techPlay.png"
                        Width="150"
                        Height="150"
                        HorizontalAlignment="Center"
                        RenderOptions.BitmapInterpolationMode="HighQuality"
                        DockPanel.Dock="Top"
                        Margin="0,0,0,15">
                        <Image.Effect>
                            <DropShadowEffect
                                Color="Black"
                                BlurRadius="15"
                                OffsetX="3"
                                OffsetY="3"/>
                        </Image.Effect>
                    </Image>

                    <!-- T√≠tulo -->
                    <TextBlock
                        Text="Fa√ßa seu login para jogar"
                        FontSize="26"
                        FontWeight="Bold"
                        Foreground="White"
                        HorizontalAlignment="Center"
                        Margin="0,0,0,15"
                        DockPanel.Dock="Top"
                    />

                    <!-- Caixa de informa√ß√µes -->
                    <Border
                        CornerRadius="12"
                        Background="#2A2A5A"
                        Padding="15,8"
                        Margin="0,10,0,0"
                        DockPanel.Dock="Bottom">
                        <StackPanel Spacing="4">
                            <TextBlock
                                Text="üí° Como jogar:"
                                FontSize="15"
                                FontWeight="Bold"
                                Foreground="#B794F6"
                                HorizontalAlignment="Center"
                            />
                            <TextBlock
                                FontSize="14"
                                Foreground="#E2E8F0"
                                HorizontalAlignment="Center"
                                TextAlignment="Center"
                                TextWrapping="Wrap">
                                <TextBlock.Text>1. Digite seu nome&#x0a;2. Digite o c√≥digo do bilhete&#x0a;3. Escolha um jogo na direita&#x0a;4. Clique em 'Entrar no Jogo'</TextBlock.Text>
                            </TextBlock>
                        </StackPanel>
                    </Border>

                    <!-- Rankings -->
                    <StackPanel DockPanel.Dock="Bottom" Spacing="10" Margin="0,10,0,15">
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="8">
                            <TextBlock Text="üèÜ" FontSize="22" VerticalAlignment="Center" />
                            <TextBlock
                                Text="Top Rankings"
                                FontSize="22"
                                FontWeight="Bold"
                                Foreground="White"
                                VerticalAlignment="Center"
                            />
                        </StackPanel>

                        <Border
                            CornerRadius="12"
                            BorderBrush="#5E60CE"
                            BorderThickness="1">
                            <ScrollViewer
                                Height="150"
                                VerticalScrollBarVisibility="Auto"
                                Padding="8">
                                <StackPanel x:Name="RankingsPanel" Spacing="6">
                                    <!-- Rankings ser√° populado dinamicamente -->
                                    <Border
                                        CornerRadius="8"
                                        Padding="10"
                                        Margin="0,1">
                                        <Border.Background>
                                            <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                                <GradientStop Color="#3A3A6B" Offset="0"/>
                                                <GradientStop Color="#4A4A7B" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Border.Background>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto" />
                                                <ColumnDefinition Width="*" />
                                                <ColumnDefinition Width="Auto" />
                                            </Grid.ColumnDefinitions>
                                            <TextBlock
                                                Grid.Column="0"
                                                Text="ü•á"
                                                FontSize="20"
                                                VerticalAlignment="Center"
                                            />
                                            <StackPanel Grid.Column="1" Margin="10,0">
                                                <TextBlock
                                                    Text="Carregando rankings..."
                                                    Foreground="White"
                                                    FontWeight="Medium"
                                                />
                                                <TextBlock
                                                    Text="Aguarde..."
                                                    Foreground="#CBD5E0"
                                                    FontSize="12"
                                                />
                                            </StackPanel>
                                            <TextBlock
                                                Grid.Column="2"
                                                Text="---"
                                                Foreground="#E2E8F0"
                                                FontWeight="Bold"
                                                VerticalAlignment="Center"
                                            />
                                        </Grid>
                                    </Border>
                                </StackPanel>
                            </ScrollViewer>
                        </Border>
                    </StackPanel>

                    <!-- Formul√°rio de Login -->
                    <StackPanel Spacing="15">
                        <!-- Name Input -->
                        <StackPanel x:Name="NamePanel" Spacing="8">
                            <StackPanel Orientation="Horizontal" Spacing="8">
                                <TextBlock Text="üë§" FontSize="22" VerticalAlignment="Center" />
                                <TextBlock
                                    Text="Seu Nome:"
                                    FontSize="18"
                                    FontWeight="Medium"
                                    Foreground="#E2E8F0"
                                    VerticalAlignment="Center"
                                />
                            </StackPanel>
                            <TextBox
                                x:Name="NameTextBox"
                                Watermark="Digite seu nome aqui..."
                                Padding="16,12"
                                FontSize="18"
                                CornerRadius="12"
                                Foreground="White"
                                BorderBrush="#5E60CE"
                                BorderThickness="2"
                                KeyDown="NameTextBox_KeyDown">
                                <TextBox.Background>
                                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                        <GradientStop Color="#2D2D5C" Offset="0"/>
                                        <GradientStop Color="#3A3A6B" Offset="1"/>
                                    </LinearGradientBrush>
                                </TextBox.Background>
                            </TextBox>
                        </StackPanel>

                        <!-- Code Input -->
                        <StackPanel Spacing="8">
                            <StackPanel Orientation="Horizontal" Spacing="8">
                                <TextBlock Text="üîë" FontSize="22" VerticalAlignment="Center" />
                                <TextBlock
                                    Text="C√≥digo de Acesso:"
                                    FontSize="18"
                                    FontWeight="Medium"
                                    Foreground="#E2E8F0"
                                    VerticalAlignment="Center"
                                />
                            </StackPanel>
                            <TextBox
                                x:Name="CodeTextBox"
                                Watermark="Digite o c√≥digo..."
                                Padding="16,12"
                                FontSize="20"
                                FontWeight="Bold"
                                CornerRadius="12"
                                Foreground="White"
                                BorderBrush="#5E60CE"
                                BorderThickness="2"
                                HorizontalContentAlignment="Center"
                                TextChanged="CodeTextBox_TextChanged"
                                KeyDown="CodeTextBox_KeyDown">
                                <TextBox.Background>
                                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                        <GradientStop Color="#2D2D5C" Offset="0"/>
                                        <GradientStop Color="#3A3A6B" Offset="1"/>
                                    </LinearGradientBrush>
                                </TextBox.Background>
                            </TextBox>
                        </StackPanel>

                        <!-- Status Message -->
                        <TextBlock
                            x:Name="StatusText"
                            Text=""
                            FontSize="16"
                            FontWeight="Medium"
                            HorizontalAlignment="Center"
                            TextAlignment="Center"
                            TextWrapping="Wrap"
                            Foreground="#4FACFE"
                            MinHeight="40"
                        />

                        <!-- Login Button -->
                        <Button
                            x:Name="LoginButton"
                            Content="üöÄ Entrar no Jogo"
                            Click="LoginButton_Click"
                            Padding="25,16"
                            FontSize="20"
                            FontWeight="Bold"
                            CornerRadius="14"
                            Foreground="White"
                            HorizontalAlignment="Stretch"
                            HorizontalContentAlignment="Center"
                            Margin="0,5">
                            <Button.Background>
                                <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                    <GradientStop Color="#5E60CE" Offset="0"/>
                                    <GradientStop Color="#7400B8" Offset="1"/>
                                </LinearGradientBrush>
                            </Button.Background>
                            <Button.Effect>
                                <DropShadowEffect BlurRadius="15" OffsetX="0" OffsetY="4" Color="#60000000"/>
                            </Button.Effect>
                        </Button>

                        <!-- Admin Panel - Escondido por padr√£o -->
                        <Border
                            x:Name="AdminPanel"
                            CornerRadius="12"
                            Padding="15"
                            IsVisible="False"
                            Margin="0,10,0,0">
                            <Border.Background>
                                <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                    <GradientStop Color="#3A3A6B" Offset="0"/>
                                    <GradientStop Color="#4A4A7B" Offset="1"/>
                                </LinearGradientBrush>
                            </Border.Background>
                            <StackPanel Spacing="12">
                                <TextBlock
                                    Text="üîß Ferramentas de Administrador"
                                    FontSize="18"
                                    FontWeight="Bold"
                                    Foreground="#E2E8F0"
                                    HorizontalAlignment="Center"
                                />
                                <Button
                                    x:Name="GenerateCodesButton"
                                    Content="üìÑ Gerar C√≥digos de Cliente"
                                    Click="GenerateCodesButton_Click"
                                    Padding="16,12"
                                    FontSize="16"
                                    CornerRadius="10"
                                    Foreground="White"
                                    HorizontalAlignment="Stretch"
                                    HorizontalContentAlignment="Center">
                                    <Button.Background>
                                        <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                            <GradientStop Color="#5E60CE" Offset="0"/>
                                            <GradientStop Color="#7400B8" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                            </StackPanel>
                        </Border>
                    </StackPanel>
                </DockPanel>
            </Border>

            <!-- Painel Direito - Game Selection -->
            <Border
                Grid.Column="1"
                CornerRadius="20"
                Margin="15,0,0,0">
                <Border.Background>
                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                        <GradientStop Color="#1E1E3F" Offset="0"/>
                        <GradientStop Color="#2A2A5A" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
                <Border.Effect>
                    <DropShadowEffect BlurRadius="20" OffsetX="0" OffsetY="10" Color="#30000000"/>
                </Border.Effect>

                <DockPanel Margin="25">
                    <!-- Game Selection Header -->
                    <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" HorizontalAlignment="Center" Spacing="12" Margin="0,0,0,15">
                        <TextBlock Text="üéÆ" FontSize="28" VerticalAlignment="Center" />
                        <TextBlock
                            Text="Escolha seu Desafio"
                            FontSize="28"
                            FontWeight="Bold"
                            Foreground="White"
                            VerticalAlignment="Center"
                        />
                    </StackPanel>

                    <!-- Game Content Grid -->
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <!-- Game List - Lado Esquerdo -->
                        <DockPanel Grid.Column="0" Margin="0,0,10,0">
                            <TextBlock
                                Text="Jogos Dispon√≠veis"
                                FontSize="18"
                                FontWeight="Bold"
                                Foreground="#E2E8F0"
                                HorizontalAlignment="Center"
                                Margin="0,0,0,10"
                                DockPanel.Dock="Top"
                            />

                            <Border
                                DockPanel.Dock="Bottom"
                                x:Name="SelectedGameBorder"
                                Background="#065F46"
                                BorderBrush="#10B981"
                                BorderThickness="2"
                                CornerRadius="12"
                                Padding="15"
                                IsVisible="False"
                                Margin="0,10,0,0">
                                <StackPanel Spacing="8">
                                    <TextBlock
                                        x:Name="SelectedGameTitle"
                                        Text="üéØ Jogo Selecionado"
                                        FontSize="16"
                                        FontWeight="Bold"
                                        Foreground="#10B981"
                                        HorizontalAlignment="Center"
                                    />
                                    <TextBlock
                                        x:Name="SelectedGameChallenge"
                                        Text="Desafio: Selecione um jogo"
                                        FontSize="14"
                                        Foreground="White"
                                        HorizontalAlignment="Center"
                                        TextWrapping="Wrap"
                                        TextAlignment="Center"
                                    />
                                </StackPanel>
                            </Border>

                            <Border
                                CornerRadius="14"
                                BorderBrush="#5E60CE"
                                BorderThickness="1"
                                Padding="15">
                                <Border.Background>
                                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                        <GradientStop Color="#1E1E3F" Offset="0"/>
                                        <GradientStop Color="#252550" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>

                                <StackPanel x:Name="GameModePanel" Spacing="15">
                                    <ScrollViewer VerticalScrollBarVisibility="Auto">
                                        <StackPanel x:Name="GameCardsPanel" Spacing="8">
                                            <!-- Game cards will be populated dynamically -->
                                        </StackPanel>
                                    </ScrollViewer>
                                </StackPanel>
                            </Border>
                        </DockPanel>

                        <!-- Instru√ß√µes e Controles - Lado Direito -->
                        <DockPanel Grid.Column="1" Margin="10,0,0,0">
                            <Border
                                DockPanel.Dock="Top"
                                CornerRadius="14"
                                BorderBrush="#10B981"
                                BorderThickness="1"
                                Padding="15"
                                Margin="0,0,0,15">
                                <Border.Background>
                                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                        <GradientStop Color="#1E1E3F" Offset="0"/>
                                        <GradientStop Color="#252550" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>

                                <DockPanel>
                                    <StackPanel DockPanel.Dock="Top" Orientation="Horizontal" HorizontalAlignment="Center" Spacing="8" Margin="0,0,0,10">
                                        <TextBlock Text="üìñ" FontSize="20" VerticalAlignment="Center" />
                                        <TextBlock
                                            Text="Instru√ß√µes do Jogo"
                                            FontSize="20"
                                            FontWeight="Bold"
                                            Foreground="White"
                                            VerticalAlignment="Center"
                                        />
                                    </StackPanel>

                                    <Border
                                        BorderBrush="#10B981"
                                        BorderThickness="1"
                                        CornerRadius="8"
                                        Padding="12">
                                        <ScrollViewer MaxHeight="200" VerticalScrollBarVisibility="Auto">
                                            <StackPanel Spacing="10">
                                                <TextBlock
                                                    x:Name="GameTitleText"
                                                    Text="üéØ Selecione um Desafio"
                                                    FontSize="18"
                                                    FontWeight="Bold"
                                                    Foreground="#10B981"
                                                    HorizontalAlignment="Center"
                                                />

                                                <TextBlock
                                                    x:Name="GameInstructionsText"
                                                    LineHeight="22"
                                                    TextWrapping="Wrap"
                                                    Foreground="#E2E8F0"
                                                    FontSize="15"
                                                    Margin="0,3,0,0"
                                                    Text="Escolha um dos desafios dispon√≠veis para ver as instru√ß√µes espec√≠ficas e come√ßar a jogar!"
                                                />
                                            </StackPanel>
                                        </ScrollViewer>
                                    </Border>
                                </DockPanel>
                            </Border>

                            <!-- Teclado Visual -->
                            <Border
                                CornerRadius="14"
                                BorderBrush="#5E60CE"
                                BorderThickness="1"
                                Padding="18">
                                <Border.Background>
                                    <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
                                        <GradientStop Color="#1E1E3F" Offset="0"/>
                                        <GradientStop Color="#252550" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>

                                <StackPanel Spacing="15">
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="10">
                                        <TextBlock Text="‚å®Ô∏è" FontSize="24" VerticalAlignment="Center" />
                                        <TextBlock
                                            Text="Teclado Visual"
                                            FontSize="24"
                                            FontWeight="Bold"
                                            Foreground="White"
                                            VerticalAlignment="Center"
                                        />
                                    </StackPanel>

                                    <TextBlock
                                        Text="Use estas teclas para controlar os LEDs:"
                                        FontSize="16"
                                        Foreground="#E2E8F0"
                                        HorizontalAlignment="Center"
                                        TextAlignment="Center"
                                        Margin="0,0,0,8"
                                    />

                                    <!-- Teclado Visual - Ajustado para espa√ßo dispon√≠vel -->
                                    <Grid HorizontalAlignment="Center" RowDefinitions="Auto,Auto,Auto,Auto" ColumnDefinitions="Auto,Auto,Auto,Auto">
                                        <!-- Row 1 - Red LEDs -->
                                        <Button Grid.Row="0" Grid.Column="0" Content="W" Background="#FF6B6B" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="0" Grid.Column="1" Content="E" Background="#FF6B6B" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="0" Grid.Column="2" Content="R" Background="#FF6B6B" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="0" Grid.Column="3" Content="T" Background="#FF6B6B" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>

                                        <!-- Row 2 - Yellow LEDs -->
                                        <Button Grid.Row="1" Grid.Column="0" Content="S" Background="#F6E05E" Foreground="Black" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="1" Grid.Column="1" Content="D" Background="#F6E05E" Foreground="Black" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="1" Grid.Column="2" Content="F" Background="#F6E05E" Foreground="Black" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="1" Grid.Column="3" Content="G" Background="#F6E05E" Foreground="Black" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>

                                        <!-- Row 3 - Green LEDs -->
                                        <Button Grid.Row="2" Grid.Column="0" Content="Y" Background="#48BB78" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="2" Grid.Column="1" Content="U" Background="#48BB78" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="2" Grid.Column="2" Content="I" Background="#48BB78" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="2" Grid.Column="3" Content="O" Background="#48BB78" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>

                                        <!-- Row 4 - Blue LEDs -->
                                        <Button Grid.Row="3" Grid.Column="0" Content="H" Background="#4299E1" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="3" Grid.Column="1" Content="J" Background="#4299E1" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="3" Grid.Column="2" Content="K" Background="#4299E1" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                        <Button Grid.Row="3" Grid.Column="3" Content="L" Background="#4299E1" Foreground="White" FontWeight="Bold" Margin="6" Padding="16" MinWidth="58" MinHeight="58" CornerRadius="10" FontSize="18"/>
                                    </Grid>

                                    <TextBlock
                                        Text="As cores correspondem √†s fileiras dos LEDs no jogo"
                                        FontSize="16"
                                        Foreground="#E2E8F0"
                                        HorizontalAlignment="Center"
                                        TextAlignment="Center"
                                        Margin="0,8,0,0"
                                    />
                                </StackPanel>
                            </Border>
                        </DockPanel>
                    </Grid>
                </DockPanel>
            </Border>
        </Grid>
    </Grid>
</Window>
